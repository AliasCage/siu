<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--Повторно используемые подсхемы-->
	<xs:include schemaLocation="_AllComlpexTypeExtention.xsd"/>
	<!--Служебные типы данных-->
	<xs:include schemaLocation="../SubSchemas/R_CommonComplexType.xsd"/>
	<!--Справочник способов образования-->
	<xs:include schemaLocation="../SubSchemas/dAgentKind.xsd"/>
	<xs:include schemaLocation="../SubSchemas/dDeclarantKind.xsd"/>
	<xs:include schemaLocation="../SubSchemas/dUnit.xsd"/>
	<!--Справочники для типа объекта выспики о правах субъекта-->
	<!--2012.04.03   Схема запроса сведений ГРП. Версия 1.16-->
	<!--                   Для ветки "RequestGRP/Request/RequiredData/RequiredDataDocument/ContentDocument/" запроса документа в качестве объекта запроса оставлен только объект недвижимости, субъект права убран.-->
	<!--2012.03.27   Схема запроса сведений ГРП. Версия 1.15-->
	<!--                   Добавлен атрибут "RequestGRP/Request/Declarant/@signatured" Для портальных заявок этот атрибут означал, что заявка была подписана ЭЦП и ЭЦП успешно проверена. В этом случае для выписок по объекту в случае, если заявитель совпадает с правообладателем, необходимо было формировать полную выписку с паспортными данными правообладателя.-->
	<!--2012.03.23   Схема запроса сведений ГРП. Версия 1.14-->
	<!--                    Убрана обязательность указания адреса у физического лица в случае предоставления выписки о правах субъекта  на объекты недвижимости-->
	<!--2012.03.20   Схема запроса сведений ГРП. Версия 1.13-->
	<!--                    Изменен классификатор Субъекты РФ-->
	<!--2012.03.12   Схема запроса сведений ГРП. Версия 1.12-->
	<!--                   Добавлен у ЮЛ как заявителя документ, подтверждающий регистрацию юридического лица -->
	<!--2012.03.11   Схема запроса сведений ГРП. Версия 1.11-->
	<!--                    Наличие представителя у Органа власти – необязателен. В случае указания представителя, наличие приложенного документа подтверждающего его полномочия – необязательно. -->
	<!--                    Добавлена возможность указания "Право на безвозмездное предоставление сведений в соответствии со статьей 8 Закона о регистрации (122-ФЗ)" RequestGRP/Request/Payment/Free-->
	<!--2012.02.20   Схема запроса сведений ГРП. Версия 1.10-->
	<!--                    Добавлен элемент RequestGRP/Request/RequiredData/RequiredDataIncapacity/IncapacityOwner/@id_db_egrp для указания ПК БД ГРП описания правообладателя-->
	<!--                    Добавлена возможность указать несколько элементов RequestGRP/Request/RequiredData/RequiredDataDocument/ContentDocument/Details/Objects -->
	<!--                            описания объектов недвижимости в запросе содержания правоустанавливающих документов  -->
	<!--                    Добавлена возможность указать несколько элементов RequestGRP/Request/RequiredData/RequiredDataDocument/ContentDocument/Details/Owners -->
	<!--                            описания правообладателей в запросе содержания правоустанавливающих документов  -->
	<!--2012.02.16   Схема запроса сведений ГРП. Версия 1.09-->
	<!--                    Добавлен элемент RequestGRP/Request/RequiredData/RequiredDataIncapacity описания правобладателя для запроса выписки из ЕГРП о признании правообладателя недееспособным или ограниченно дееспособным-->
	<!--2012.02.06   Схема запроса сведений ГРП. Версия 1.08-->
	<!--                    tArea удален элемент AreaCode - значение площади в описании объекта запроса допускается указать только одно-->
	<!--                    Удален справоник dArea.xsd-->
	<!--                    dDeclarantKind.xsd и dAgentKind.xsd перенесены  в ../SubSchemas-->
	<!--2012.02.03   Схема запроса сведений ГРП. Версия 1.07-->
	<!--                    Добавлен GUID-->
	<!--                    В платежных документа сумма в копейках-->
	<!--                    tFIO	изменена размерность фамилии = 100 и размерность имени = 45 знаков-->
	<!--2012.02.02   Схема запроса сведений ГРП. Версия 1.06-->
	<!--	                ExtractRealtyList - возможность указать несколько объектов-->
	<!--	                ContentDocument - возможность указать субъекта правообладателя-->
	<!--	                tDeclarant, tOwner, tAgent - добавлен атрибут id_db_egrp для указания ПК БД ЕГРП, полученные ПВД из веб-сервиса учетной системы -->
	<!--2012.02.01   Схема запроса сведений ГРП. Версия 1.05-->
	<!--	                tExtractBuilding, tExtractRoom переименованы атрибуты-->
	<!--	                tQuantityAttribute для всех атрибутов указано ограничение "больше нуля" -->
	<!--2012.02.01   Схема запроса сведений ГРП. Версия 1.04-->
	<!--                   вынесены общие элементы tExtractObjectType, tExtractSubjectObjectType, tExtractRoom, tExtractBuilding, tDopInfo-->
	<!--                   удален справочник dBuildingAssignation.xsd-->
	<!--                   везде где встречалась конструкция Object\Object первое включение переименовано в Objects-->
	<!--                   у всех типов xs:boolean задано значение по умолчанию true-->
	<!--                   для объекта MunicipalService добавлено поле должность-->
	<!--2012.01.31   Схема запроса сведений ГРП. Версия 1.03-->
	<!--                   sObjKindGRP используются коды из справочников Росреестра.-->
	<!--                   удален справочник dRoomKind.xsd-->
	<!--                   tExtractSubjectObjectType модифицирован:-->
	<!--                                                           - убраны коды для ЗУ и помещений,-->
	<!--                                                           - вид помещений задется определенным атрибутом-->
	<!--                   dRealtyGRP удалены виды "земельный участок" и "помещение". Данные типы задают специальными элементами в  tExtractSubjectObjectType-->
	<!--                   dBuildingAssignation удалены назначения для помещений -->
	<!--2012.01.30   Схема запроса сведений ГРП. Версия 1.02-->
	<!--                   tRequiredObject Исправлен режим вариантов c sequence на choice.-->
	<!--                   tRequiredObject Тип(размерность) Name указано как s4000-->
	<!--                   Справочник dUnit.xsd используется из ../SubSchemas-->
	<!--                   tOwner изменено название составных эелементов "Person" в "PersonO", "Organization" в "OrganizationO", "Governance" в "GovernanceO", "ForeignOrganization" в ForeignOrganizationO"-->
	<!--2012.01.27   Схема запроса сведений ГРП. Версия 1.01-->
	<!--                   Заявитель. Иностранное юрлицо. Представитель стал обязателен-->
	<!--                   Субъект запроса - физлицо. Орган выдачи предыдущего удостоверения личности стал необязательный-->
	<!--                   sObjKindGRP удален тип "Иной". Тип "иной" определяется если заполнено текстовое описание типа-->
	<!--                   Существенно модифицирован tRequiredObject. Повышен контроль обязательности заполнения атрибутов на уровне схемы в зависимости от типа объекта-->
	<!--                   DescribeContract перенесен из RequestGRP/Request/RequiredData/RequiredDataDocument/CopyDocument в RequestGRP/Request/RequiredData/RequiredDataDocument/ContentDocument-->
	<!--                   dFlatKind.xsd переименован в dRoomKind.xsd-->
	<!--                   dRealty.xsd переименован в dRealtyGRP.xsd, скорректировано содержание - оставлены типы объектов ГРП-->
	<!--2011.12.14 Схема запроса сведений ГРП. Версия 1-->
	<xs:element name="RequestGRP">
		<xs:annotation>
			<xs:documentation>Запрос на предоставление сведений ГРП</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="eDocument">
					<xs:annotation>
						<xs:documentation>Электронный документ</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Version" type="xs:string" use="required" fixed="1.16">
							<xs:annotation>
								<xs:documentation>Версия схемы</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="GUID" type="sGUID" use="required">
							<xs:annotation>
								<xs:documentation>Идентификатор запроса</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="Request">
					<xs:annotation>
						<xs:documentation>Запрос</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="RequiredData">
								<xs:annotation>
									<xs:documentation>Запрашиваемые сведения</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice>
										<xs:element name="RequiredDataRealty">
											<xs:annotation>
												<xs:documentation>По объекту(ам) недвижимости</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:choice>
													<xs:element name="ExtractRealty">
														<xs:annotation>
															<xs:documentation>Выписка о правах на объект недвижимости</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Objects" type="tRequiredObject" maxOccurs="unbounded">
																	<xs:annotation>
																		<xs:documentation>Объекты, по которым запрашиваются сведения</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="ExtractRealtyList">
														<xs:annotation>
															<xs:documentation>Выписка о переходе прав объекта недвижимости</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Objects" type="tRequiredObject" maxOccurs="unbounded">
																	<xs:annotation>
																		<xs:documentation>Объект</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="ExtractRealtyInfo">
														<xs:annotation>
															<xs:documentation>Справка о лицах, получивших сведения по объекту недвижимости</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Objects" type="tRequiredObject">
																	<xs:annotation>
																		<xs:documentation>Объект</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:choice>
											</xs:complexType>
										</xs:element>
										<xs:element name="RequiredDataSubject">
											<xs:annotation>
												<xs:documentation>Выписка о правах субъекта  на объекты недвижимости</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:choice>
													<xs:element name="ExtractSubjectRegion">
														<xs:annotation>
															<xs:documentation>Выписка о правах субъекта  на объекты недвижимости территории</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Owner" type="tOwner" maxOccurs="unbounded"/>
																<xs:element name="Territory">
																	<xs:annotation>
																		<xs:documentation>Территория РФ</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:choice>
																			<xs:element name="Territory_Russia" type="xs:boolean" fixed="true">
																				<xs:annotation>
																					<xs:documentation>По всей территории РФ</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="Regions">
																				<xs:annotation>
																					<xs:documentation>Список регионов РФ</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="Region" type="dRegionsRF" maxOccurs="unbounded"/>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																		</xs:choice>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="ExtractSubject">
														<xs:annotation>
															<xs:documentation>Выписка о правах субъекта  на объекты недвижимости</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Owners" type="tOwner" maxOccurs="unbounded"/>
																<xs:element name="Data_Period">
																	<xs:annotation>
																		<xs:documentation>Период времени, за который необходимо предоставить сведения</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:choice>
																			<xs:element name="Date" type="xs:date">
																				<xs:annotation>
																					<xs:documentation>На указанную дату</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="Date_Start" type="xs:date">
																				<xs:annotation>
																					<xs:documentation>С указанной даты по текущий момент</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="Date_End" type="xs:date">
																				<xs:annotation>
																					<xs:documentation>До указанной даты</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="Interval">
																				<xs:annotation>
																					<xs:documentation>Период времени, за который необходимо предоставить сведения</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="Date_Start" type="xs:date">
																							<xs:annotation>
																								<xs:documentation>Начало периода</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="Date_End" type="xs:date">
																							<xs:annotation>
																								<xs:documentation>Конец периода</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																		</xs:choice>
																	</xs:complexType>
																</xs:element>
																<xs:element name="RealtyType">
																	<xs:annotation>
																		<xs:documentation>Типы объектов недвижимости</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:choice>
																			<xs:element name="RealtyType_All" type="xs:boolean" fixed="true">
																				<xs:annotation>
																					<xs:documentation>всем типам</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="RealtyType_Text" type="tExtractSubjectObjectType" maxOccurs="unbounded">
																				<xs:annotation>
																					<xs:documentation>по указанным типам </xs:documentation>
																				</xs:annotation>
																			</xs:element>
																		</xs:choice>
																	</xs:complexType>
																</xs:element>
																<xs:element name="Territory">
																	<xs:annotation>
																		<xs:documentation>Территория РФ</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:choice>
																			<xs:element name="Territory_Russia" type="xs:boolean" fixed="true">
																				<xs:annotation>
																					<xs:documentation>По всей территории РФ</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="Regions">
																				<xs:annotation>
																					<xs:documentation>Список регионов РФ</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="Region" type="dRegionsRF" maxOccurs="unbounded"/>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																		</xs:choice>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:choice>
											</xs:complexType>
										</xs:element>
										<xs:element name="RequiredDataDocument">
											<xs:annotation>
												<xs:documentation>Сведения по документу</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:choice>
													<xs:element name="CopyDocument">
														<xs:annotation>
															<xs:documentation>Запрос копии документа

Указываются вид (наименование) и реквизиты договора (сделки)</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="tRequiredDocument">
																	<xs:sequence>
																		<xs:element name="Objects" type="tRequiredObject">
																			<xs:annotation>
																				<xs:documentation>Объект</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																	</xs:sequence>
																</xs:extension>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="ContentDocument">
														<xs:annotation>
															<xs:documentation>Справка о содержании правоустанавливающих документов 

Указываются сведения о правоустанавливающем документе (вид, реквизиты), на основании которого был зарегистрирован переход права на указанный в запросе объект недвижимости от одного лица к другому или на основании которого конкретное лицо, указанное в запросе, приобрело объект недвижимости.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="tRequiredDocument">
																	<xs:sequence>
																		<xs:element name="Objects">
																			<xs:annotation>
																				<xs:documentation>Объекты, на которые были зарегистрированы переходы права от одного субъекта к другому</xs:documentation>
																			</xs:annotation>
																			<xs:complexType>
																				<xs:sequence>
																					<xs:element name="Object" type="tRequiredObject" maxOccurs="unbounded">
																						<xs:annotation>
																							<xs:documentation>Объект, на который были зарегистрированы переходы права от одного субъекта к другому</xs:documentation>
																						</xs:annotation>
																					</xs:element>
																				</xs:sequence>
																			</xs:complexType>
																		</xs:element>
																		<xs:element name="DescribeContract" type="s500" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>положения, интересующие заявителя</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																	</xs:sequence>
																</xs:extension>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
												</xs:choice>
											</xs:complexType>
										</xs:element>
										<xs:element name="RequiredDataIncapacity">
											<xs:annotation>
												<xs:documentation>Выписка о признании правообладателя недееспособным или ограниченно дееспособным</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="IncapacityOwner" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Правообладатель</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="tPersonOwner">
																	<xs:attribute name="id_db_egrp">
																		<xs:annotation>
																			<xs:documentation>Идентификатор в базе данных ЕГПР </xs:documentation>
																		</xs:annotation>
																		<xs:simpleType>
																			<xs:restriction base="xs:string">
																				<xs:maxLength value="50"/>
																			</xs:restriction>
																		</xs:simpleType>
																	</xs:attribute>
																</xs:extension>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:choice>
								</xs:complexType>
							</xs:element>
							<xs:element name="Declarant" type="tDeclarant">
								<xs:annotation>
									<xs:documentation>Заявитель</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Payment">
								<xs:annotation>
									<xs:documentation>Оплата</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice>
										<xs:element name="Payment_Documents">
											<xs:annotation>
												<xs:documentation>Платёжные документы</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Payment_Document" type="tPayDocument" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Платёжный документ</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ReasonFree_Documents">
											<xs:annotation>
												<xs:documentation>Документы основания получение сведений без оплаты</xs:documentation>
											</xs:annotation>
											<xs:complexType>
