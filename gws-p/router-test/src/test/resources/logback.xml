<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="ru.codeinside.gws" level="TRACE"/>

  <logger name="com.sun.xml.ws.util.pipe" level="TRACE"/>

  <logger name="ru.CryptoPro" level="ERROR"/>

  <root level="info">
    <appender-ref ref="STDOUT"/>
  </root>
</configuration>